{
  "name": "flight-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon --ext js ./bin/www",
    "server-dependencies": "npm install",
    "client-dependencies": "npm install --prefix client",
    "install-all-deps": "concurrently \"npm run server-dependencies\" \"npm run client-dependencies\"",
    "client": "npm start --prefix client",
    "server": "nodemon index.js",
    "demo": "concurrently \"npm run client\" \"npm run server\""
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "fast-csv": "^4.3.3",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.31",
    "morgan": "~1.9.1"
  }
}
